t({\n  components:{Button}\n})\nexport default class Labels extends Vue {\n  tags = tagListModel.data;\n  createTag() {\n    const name = window.prompt(\"请输入标签名\");\n    if (name) {\n      const message = tagListModel.create(name);\n      if (message === \"duplicated\") {\n        window.alert(\"标签名重复了\");\n      } else if (message === \"success\") {\n        window.alert(\"标签添加成功\");\n      }\n    } else {\n      window.alert(\"标签名不能为空\");\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.tags {\n  background: white;\n  font-size: 16px;\n  padding-left: 16px;\n  > .tag {\n    min-height: 44px;\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    border-bottom: 1px solid #e6e6e6;\n    svg {\n      width: 18x;\n      height: 18px;\n      color: #666;\n      margin-right: 16px;\n    }\n  }\n}\n .createTag-wrapper {\n    text-align: center;\n    padding: 16px;\n    margin-top: 44-16px;\n  }\n\n</style>\n"]}]}