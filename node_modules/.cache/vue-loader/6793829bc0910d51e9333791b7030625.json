: flex;\n    justify-content: center;\n    align-items: center;\n    position: relative;\n    &.selected::after {\n      content: \"\";\n      position: absolute;\n      bottom: 0;\n      left: 0;\n      width: 100%;\n      height: 4px;\n      background: #333;\n    }\n  }\n}\n.notes {\n  background: #f5f5f5;\n  display: block;\n  font-size: 14px;\n  padding-left: 16px;\n  display: flex;\n  align-items: center;\n  .name {\n    padding-right: 16px;\n  }\n  input {\n    height: 64px;\n    flex-grow: 1;\n    background: transparent;\n    border: none;\n  }\n}\n.tags {\n  font-size: 14px;\n  padding: 16px;\n  > .current {\n    display: flex;\n    > li {\n      background: #d9d9d9;\n      $h: 24px;\n      height: $h;\n      line-height: $h;\n      border-radius: $h/2;\n      padding: 0 16px;\n      margin-right: 12px;\n    }\n  }\n  > .new {\n    padding-top: 16px;\n    button {\n      background: transparent;\n      border: none;\n      color: #999;\n      border-bottom: 1px solid;\n      padding: 0 4px;\n    }\n  }\n}\n</style>\n\n\n"]}]}